{
  "name": "pome_api",
  "version": "1.0.0",
  "description": "API for Portofolio of My Experiences",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix ./src/**/*.ts",
    "check-types": "tsc --noEmit",
    "build": "npm run lint && npm run check-types && npx tsc",
    "jasmine": "jasmine-ts --config=spec/jasmine.json",
    "test": "npm run db-up && npm run jasmine",
    "db-up": "db-migrate --env dev up",
    "db-down": "db-migrate --env dev down",
    "dev": "db-migrate --env test up && nodemon src/server.ts",
    "start": "node dist/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'"
  },
  "author": "Hajime Kawata",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.9",
    "@types/pg": "^7.14.7",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "db-migrate": "^1.0.0-beta.16",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.0",
    "express-rate-limit": "^5.3.0",
    "helmet": "^4.6.0",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "pg": "^8.5.1",
    "save": "^2.4.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^4.0.0",
    "@types/http-errors": "^1.8.1",
    "@types/jasmine": "^3.9.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.9.1",
    "@types/sharp": "^0.29.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jasmine": "^3.9.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^2.0.12",
    "optionator": "^0.9.1",
    "prettier": "^1.19.1",
    "supertest": "^6.1.6",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
